<odoo>
    <record id="view_meter_list_report_wizard_form" model="ir.ui.view">
        <field name="name">meter.list.report.wizard.form</field>
        <field name="model">meter.list.report.wizard</field>
        <field name="arch" type="xml">
            <form string="Print Meter List">
                <group>
                    <field name="zone_id"/>
                </group>
                <footer>
                    <button name="action_print_report" type="object" string="Print PDF" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
            
                </footer>
            </form>
        </field>
    </record>

    <record id="action_meter_list_report_wizard" model="ir.actions.act_window">
        <field name="name">Print Meter List</field>
        <field name="res_model">meter.list.report.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

      <menuitem id="menu_meter_list_report_wizard"
              name="Print Meter List"
              parent="menu_meter_reports"
              action="action_meter_list_report_wizard"
              sequence="100"/>
</odoo>